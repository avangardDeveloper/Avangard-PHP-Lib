            "GenerateBox\\": "src/generateBoxAuth/lib",
            "Box\\": "src/Lib/Box",
            "AtolV4\\": "src/Lib/atol-online/",
            "AtolV5\\": "src/Lib/atol-online-v5/",
            "OrangeDataClient\\": "src/Lib/orange-data/"
