{
    "name": "avangard/api",
    "description": "AVANGARD API v4 client for php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Bykovskiy",
            "email": "BykovskiyM@avangard.ru"
        },
        {
            "name": "Aleksander Ignatev",
            "email": "ignateva@avangard.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-dom": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.4",
        "psr/log": "^1.0.2",
        "psr/http-message": "1.0.1 as 1.0"
    },
    "autoload": {
        "psr-4": {
